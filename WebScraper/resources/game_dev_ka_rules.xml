  <relative-paths ka-path="ka/ka_gd_export.csv"/>
  <dates>
  	<date date-bound="begin" date-month="5" date-day="8" date-year="2020"/>
  </dates>
  <headers/>
  <rules>
    <rule assignment-type="Challenge">
      <completion-rule completion-points="4"/>
    </rule>
    <rule assignment-type="Project">
      <completion-rule completion-points="5"/>
    </rule>
    <rule assignment-type="Exercise">
      <points-rule>
      	<curve/>
      </points-rule>
    </rule>
  </rules>
</parsing-rules>
